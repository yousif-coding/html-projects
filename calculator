<script>
    function add(){
        var a=67
        var b=21
    document.write(a+b)
    }
        function subtract(){
        var a=67
        var b=21
    document.write(a-b)
    }
        function multiply(){
        var a=67
        var b=21
    document.write(a*b)
    }
        function divide(){
        var a=67
        var b=21
    document.write(a/b)
    }
</script>




